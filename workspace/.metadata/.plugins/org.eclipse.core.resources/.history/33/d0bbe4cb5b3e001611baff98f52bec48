package weixiang_01;

public class A {
	public void set()
	{
		System.out.println("set");
	}
	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
//		System.out.println(args[0]);
		Object o = new A();
		A a=(A)o;
		a.set();
//		A a=(A)o;    //ClassCastException 引用转换异常
	
	}

}
